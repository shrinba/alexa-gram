<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>

    <img id="img" src=<%= url %> />

  </body>

  <script src="/socket.io/socket.io.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
	<script>
  	var socket = io();

  	socket.on('image url', function(url){
  		$('#img').attr("src", url);
  	});
	</script>
</html>
